dist: 
	@mkdir -p .obj/chooser && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/chooser/ && $(COPY_FILE) --parents chooser.ui.h utregdlg.ui.h pindlg.ui.h carddlg.ui.h .obj/chooser/ && ( cd `dirname .obj/chooser` && $(TAR) chooser.tar chooser && $(GZIP) chooser.tar ) && $(MOVE) `dirname .obj/chooser`/chooser.tar.gz . && $(DEL_FILE) -r .obj/chooser

